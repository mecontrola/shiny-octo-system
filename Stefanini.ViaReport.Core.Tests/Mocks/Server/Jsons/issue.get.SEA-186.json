nks]*\r\n NA\r\n\r\n*[Dependências]*\r\n NA\r\n\r\n*[Interfaces]*\r\n NA","customfield_10010":"9223372036854775807","customfield_10011":null,"customfield_17323":{"self":"https://jira.viavarejo.com.br/rest/api/2/customFieldOption/17748","value":"Não","id":"17748"},"customfield_10012":null,"customfield_14612":null,"customfield_15701":null,"customfield_12311":null,"customfield_14613":null,"customfield_15702":[{"self":"https://jira.viavarejo.com.br/rest/api/2/customFieldOption/14715","value":"Validação/Aprendizado","id":"14715"}],"customfield_13400":null,"timetracking":{},"customfield_14605":null,"customfield_14606":null,"customfield_14604":null,"customfield_14609":null,"customfield_14607":null,"customfield_14608":null,"summary":" Implementar IOS Scroll de resultado de busca","customfield_11210":null,"customfield_10000":null,"customfield_16102":null,"customfield_10001":null,"customfield_17319":null,"customfield_10002":null,"customfield_16900":null,"customfield_17318":null,"customfield_10003":null,"customfield_17317":null,"customfield_10004":null,"customfield_17316":null,"environment":null,"customfield_15803":null,"duedate":null,"comment":{"comments":[],"maxResults":0,"total":0,"startAt":0},"customfield_17300":null,"customfield_15001":null,"customfield_15004":null,"customfield_17304":null,"customfield_17303":null,"customfield_17302":null,"fixVersions":[{"self":"https://jira.viavarejo.com.br/rest/api/2/version/21127","id":"21127","description":"Android 8.5.46 | iOS 7.8.46","name":"Delivery V1.4","archived":false,"released":true,"releaseDate":"2022-01-23"}],"customfield_10110":"16110_*:*_1_*:*_49082790_*|*_16114_*:*_1_*:*_151957_*|*_16113_*:*_1_*:*_81136230_*|*_16112_*:*_1_*:*_41984_*|*_16111_*:*_1_*:*_19610_*|*_10514_*:*_1_*:*_11497_*|*_12527_*:*_1_*:*_459133537_*|*_13104_*:*_1_*:*_86916843_*|*_12100_*:*_1_*:*_7740_*|*_10002_*:*_1_*:*_134670_*|*_16115_*:*_1_*:*_69107010_*|*_11601_*:*_1_*:*_436049256_*|*_10214_*:*_1_*:*_0","customfield_15003":null,"customfield_17301":null,